{
  "name": "@jacrys/vuemapgl",
  "version": "0.1.0",
  "keywords": [
    "vue",
    "vue3",
    "vuejs",
    "vue3js",
    "component",
    "vue-component",
    "vue3-component",
    "maplibre",
    "maplibre-gl",
    "maplibre-gl-js",
    "mapbox",
    "mapbox-gl",
    "mapbox-gl-js",
    "map"
  ],
  "author": "Jacrys <kclewis0614@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/jacrys/vuemapgl.git"
  },
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "main": "dist/vuemapgl.umd.js",
  "browser": "dist/vuemapgl.es.js",
  "module": "dist/vuemapgl.es.js",
  "types": "dist/main.d.ts",
  "web-types": "dist/web-types.json",
  "files": [
    "dist/**/*",
    "lib/**/*"
  ],
  "scripts": {
    "build": "run-s type-check build-only docs:gen-web-types",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "type-build": "vue-tsc --build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:gen-web-types": "vue-docgen-web-types --configFile web-types.config.cjs",
    "lint:oxlint": "oxlint ltb --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint lib --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write lib/"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@babel/types": "^7.23.9",
    "@tsconfig/node22": "^22.0.0",
    "@types/geojson": "^7946.0.16",
    "@types/geojson-vt": "^3.2.5",
    "@types/jsdom": "^21.1.7",
    "@types/mapbox-gl": "^3.4.1",
    "@types/node": "^22.13.1",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/eslint-plugin": "1.1.25",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.18.0",
    "eslint-plugin-oxlint": "^0.15.6",
    "eslint-plugin-vue": "^9.32.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.15.6",
    "prettier": "^3.4.2",
    "typescript": "~5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-vue-devtools": "^7.7.1",
    "vitest": "^3.0.5",
    "vue-tsc": "^2.2.0"
  }
}
